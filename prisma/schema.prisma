generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model co2 {
  id         Int    @id
  country    String
  iso_code   String
  year       Int
  total      Float
  coal       Float
  oil        Float
  gas        Float
  cement     Float
  flaring    Float
  other      Float
  per_capita Float
}
